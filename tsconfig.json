{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "./dist",
        "sourceMap": true,
        "target": "es5",
        "strictNullChecks": false,
        "declaration": true,
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2016",
            "dom"
        ],
        "types": [
            "phaser-ce"
        ]
    },
    "include": [
        "src"
    ],
    "exclude": [
        "./node_modules"
    ]
}